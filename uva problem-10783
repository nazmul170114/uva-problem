#include<cstdio>
using namespace std;
int main()
{
    int a,b,t,i,j,start,end,odd;
    scanf("%d",&t);
    for(i=1;i<=t;i++){

        odd=0;
        scanf("%d%d",&a,&b);
        if(a<b){

            start=a;
            end=b;
        }

        else if(a>b){

            start=b;
            end=a;
        }

        for(j=start;j<=end;j++){

            if(j%2==1){

                odd=odd+j;

            }

        }


        printf("Case %d: %d\n",i,odd);
       


    }




    return 0;
}
