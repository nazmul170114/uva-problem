#include<cstdio>
using namespace std;
int main()
{
    int i,j,t,n,a,b,c;
    long long summed_burden;
    scanf("%d",&t);
    for(i=1;i<=t;i++){
        scanf("%d",&n);
        summed_burden=0;
        for(j=1;j<=n;j++){

            scanf("%d%d%d",&a,&b,&c);

            summed_burden=summed_burden+(a*c);
        }

        printf("%lld\n",summed_burden);

    }





    return 0;
}
