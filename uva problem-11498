#include<cstdio>
using namespace std;
int main()
{
    int k,m,n,x,y,i;
    while(scanf("%d",&k)!=EOF){

         scanf("%d%d",&m,&n);

    for(i=1;i<=k;i++){

          scanf("%d%d",&x,&y);
          if(x==m || y==n){

            printf("divisa\n");
          }

          else if(x<m && y>n){

            printf("NO\n");
          }

          else if(x>m && y>n){

            printf("NE\n");
          }

          else if(x>m && y<n){

            printf("SE\n");
          }

          else if(x<m && y<n){

            printf("SO\n");
          }



       }

   }



    return 0;
}
